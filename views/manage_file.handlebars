<div id="manage_file_div">
  <h1>Manage File</h1>
  <h3>File: {file_name}</h3>
  <h3>Owner: {username}</h3>
  <div id="center">
    <div class="vertical_navbar">
      <form method="get">
        <button formaction="/file/{{data.file_id}}/manage/users">Users</button>
        <button formaction="/file/{{data.file_id}}/manage/invites">Invites</button>
      </form>
    </div>
    <div class="content">
      {{#if data.users}}
        {{> file_users this}}
      {{else if data.invites}}
        {{> file_invites this}}
      {{/if}}

    </div>
  </div>
</div>
